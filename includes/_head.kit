	<script>
jQuery(function(){
			
			jQuery('#camera_wrap_1').camera({
				alignment: 'center',
				barPosition: 'top',
				thumbnails: false,
				fx: 'simpleFade',
				height: '27%',
				pagination: true

			});

		});
	</script>

	<style>
#back_to_camera {
			clear: both;
			display: block;
			height: 80px;
			line-height: 40px;
			padding: 20px;
			z-index: -20;
		}
		.fluid_container {
			margin: 0 auto;
			/*max-width: 1000px;*/
			width: 100%;
			z-index: -10;
		}
		.camera_wrap .camera_pag .camera_pag_ul {
			position: absolute;
			top:206px;
			right:10px;
		}
		.camera_wrap .camera_pag .camera_pag_ul li {
		-webkit-border-radius: 5px;
		-moz-border-radius: 5px;
		border-radius: 5px;
		cursor: pointer;
		display: inline-block;
		height: 10px;
		margin: 20px 5px;
		position: relative;
		text-align: left;
		text-indent: -9999px;
		width: 10px;
	}
		.camera_wrap .camera_pag .camera_pag_ul li > span {
		-webkit-border-radius: 5px;
		-moz-border-radius: 5px;
		border-radius: 5px;
		height: 6px;
		left: 2px;
		overflow: hidden;
		position: absolute;
		top: 2px;
		width: 6px;
}
	</style>

